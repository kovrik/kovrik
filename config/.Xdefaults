Xcursor.theme: Neutral_Plus
 
Xft.dpi: 90.0
! Xft.autohint: false
! Xft.hinting: true
! Xft.hintstyle: hintfull
Xft.antialias: true
! Xft.rgba: rgb
! Xft.lcdfilter: lcddefault
 
! URxvt
! URxvt*font:                     xft:liberation mono:size=12
urxvt*font:                     xft:Liberation\ Mono:size=13
 
URxvt*scrollBar:                false
URxvt*saveLines:                3200
 
URxvt.perl-ext-common:          default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:      firefox
URxvt.url-select.underline:     true
URxvt.keysym.M-u:               perl:url-select:select_next
URxvt.keysym.M-Escape:          perl:keyboard-select:activate
URxvt.keysym.M-s:               perl:keyboard-select:search
 
URxvt*tabbed.tabbar-bg: 0
URxvt*tabbed.tabbar-fg: 11
URxvt*tabbed.tab-bg:    11
URxvt*tabbed.tab-fg:    0

! Tango color palette
URxvt*background: #000000
URxvt*foreground: #ffffff
URxvt*cursorColor: #ffffff
! foreground color for underline
URxvt*colorUL: #8ae234
! line color for underline
URxvt*underlineColor: #92659a
! black dark/light
URxvt*color0: #2e3436
URxvt*color8: #6e706b
! red dark/light
URxvt*color1: #cc0000
URxvt*color9: #ef2929
! green dark/light
URxvt*color2: #4e9a06
URxvt*color10: #8ae234
! yellow dark/light
URxvt*color3: #edd400
URxvt*color11: #fce94f
! blue dark/light
URxvt*color4: #3465a4
URxvt*color12: #729fcf
! magenta dark/light
